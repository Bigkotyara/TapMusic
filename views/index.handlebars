<!doctype html>

<html>
<head>
	<title>TapMusic</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-alpha.4/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/introHCI.css" rel="stylesheet">
	<link href="https://afeld.github.io/emoji-css/emoji.css" rel="stylesheet">
	<link href="/css/login.css" rel="stylesheet">
	<!-- this is a comment in HTML -->

	<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
	<!--[if lt IE 9]>
	<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
	<script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
	<![endif]-->
</head>

<body>
	<div class="container">
		<nav class="topnav">

			<h3 align="center">TapMusic</h3>
			<h3><a class="topright" href="tutorial/"><span class="glyphicon glyphicon-info-sign"></span></a></h3>
		</nav>
		<br>
		<div class="project">
			{{#each projects}}
			<div id={{id}} class="emoji-button">
				<button class="{{icon}}" id="{{name}}">
				</button>
			</div>
			{{/each}}
		</div>
		<div class="project">
			{{#each projects1}}
			<div id={{id}} class="emoji-button">
				<button class="{{icon}}" id="{{name}}">
				</button>
			</div>
			{{/each}}
		</div>
		<div class="project">
			{{#each projects2}}
			<div id={{id}} class="emoji-button">
				<button class="{{icon}}" id="{{name}}">
				</button>
			</div>
			{{/each}}
		</div>
		<div class="botnav">
			<div>
				<span id="song-up" class="glyphicon glyphicon-chevron-up"></span>
				<h2 id="song-title">Tap an Icon to Begin&nbsp</h2>
				<button id="play-button" type="button" class="btn btn-default btn-lg"><span class="glyphicon glyphicon-play"></span></button>&nbsp<button id="next-button" type="button" class="btn btn-default btn-lg"><span class="glyphicon glyphicon-forward"></span></button>
			</div>
		</div>

	</div>


	<div id="myNav" class="overlay">

		<div class="overlay-content">
			<!-- <span id="song-down" class="glyphicon glyphicon-chevron-down closebtn"></span> -->
			<span id="song-down" class="closebtn"><span class="glyphicon glyphicon-chevron-down" ></span></span>

			<div class="dropdown show topright">
				<button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
					<span class="glyphicon glyphicon-list"></span>
				</button>
				<ul class="dropdown-menu dropdown-menu-right pull-right">
					<li><a onclick="document.getElementById('login-pop').style.display='block'" style="width:auto;"><h4>Filters</h4></a></li>
					<li role="separator" class="divider"></li>
					<li><a href="/tutorial/"><h4>Help</h4></a></li>
				</ul>
			</div>
			<!--mood header-->
			<h2 id="category_header"></h2>
			<div>
				<!--album art--><img class="resize" src="http://www.firesidechatmusic.com/artwork/AlbumArt_Blank.jpg" alt="album art">
			</div>
			<div class="progress">
				<div id="length" class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width:0%">
					<span class="sr-only"></span>
				</div>
			</div>

			<div>
				<h4 id='song-title-ov' style="color: white">Song Title</h4>
				<h4 id='song-artist-ov' style="color: white">&nbsp</h4>
			</div>

			<div class="media-buttons">
				<button class="media-button" id="play-button-ov"><i class="glyphicon glyphicon-play"></i></button>&nbsp<button class="media-button" id="next-button-ov"><i class="glyphicon glyphicon-forward"></i></button>
			</div>
			<input id="vol-control" type="range" min="0" max="100" step="1" oninput="SetVolume(this.value)" onchange="SetVolume(this.value)"></input>
</div>
</div>
<div id="login-pop" class="modal">
	<div class="row animate">
		<div class="col-md-6 col-md-offset-3">
			<div class="panel panel-login">
				<span onclick="document.getElementById('login-pop').style.display='none'" class="close2" title="Close Modal">&times;</span>
				<div class="panel-heading">
					<div class="row">
						<a href="#" class="active" id="filter-form-link">Filter</a>
					</div>
					<hr>
				</div>
				<div class="panel-body">
					<div class="row">
						<div class="col-lg-12">
								<div class="form-group">
										<div class="chips chips-autocomplete"></div>
									<div class="form-group text-center">
										<input type="checkbox" tabindex="3" class="" name="remember" id="remember">
										<label for="remember"> Remember My Preferences</label>
									</div>
								<!-- <form id="register-form" method="post" role="form" style="display: none;"> -->
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!--</div>-->
		<!-- </form>-->
	</div>

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/introHCI.js"></script>
	<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
	<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script src="/js/tags.js"></script>
	<script src="/js/tagger.js"></script>
</body>
</html>
